local function GiveGodMode(char)
	
	if Settings.GodMode then
		for _, bp in pairs(char:GetChildren()) do
			
			if bp:IsA("BasePart") then
			
				bp.CanTouch = false;
			end

		end

	else
		
		for _, bp in pairs(char:GetChildren()) do
			
			if bp:IsA("BasePart") then
			
				bp.CanTouch = true;
			end

		end

	end
end

local function GiveNoClip(char)
	
	if Settings.NoClip then
		for _, bp in pairs(char:GetChildren()) do
			
			if bp:IsA("BasePart") then
			
				bp.CanCollide = false;
			end

		end

	else
		
		for _, bp in pairs(char:GetChildren()) do
			
			if bp:IsA("BasePart") then
			
				bp.CanCollide = true;
			end

		end

	end
end

local function ChangeSpeed()

	local Hum = LocalPlayer.Character:FindFirstChild("Humanoid")
	

	if Hum then
		LocalPlayer.Character.Humanoid:GetPropertyChangedSignal'WalkSpeed':Connect(function()
			LocalPlayer.Character.Humanoid.WalkSpeed = Settings.Speed;
		end)
		
		LocalPlayer.Character.Humanoid.WalkSpeed = Settings.Speed

		
	end
	
end

local function ChangeJumpHeight()

	local Hum = LocalPlayer.Character:FindFirstChild("Humanoid")
	

	if Hum then
		LocalPlayer.Character.Humanoid:GetPropertyChangedSignal'JumpHeight':Connect(function()
			LocalPlayer.Character.Humanoid.JumpHeight = Settings.JumpHeight;
		end)
		
		LocalPlayer.Character.Humanoid.JumpHeight = Settings.JumpHeight

		
	end
	
end

coroutine.wrap(function()
	while true do
		
		
	GiveGodMode(LocalPlayer.Character);
	GiveNoClip(LocalPlayer.Character);
	Collect();
	Upgrade();
	ChangeJumpHeight();
	ChangeSpeed();
		
	task.wait(.1);

	end
end)()
