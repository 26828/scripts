local InfoTab = Window:CreateTab("Info", 4483362458)
local MainTab = Window:CreateTab("Main", 4483362458)
local MacroTab = Window:CreateTab("Macro", 4483362458)
local MiscTab = Window:CreateTab("Misc", 4483362458)

local Settings = {

	
	GodMode = false,
	NoClip = false,
	Collect = "None",
	Upgrade = "None",
	Speed = 16,
	JumpHeight = 10,
	MacroCollect = "None",
	MacroUpgrade = "None"
}

MiscTab:CreateSlider({
	Name = "Speed",
	Range = {0, 1000},
	Increment = 1,
	CurrentValue = Settings.Speed,
	Callback = function(v)
		Settings.Speed = v
	end
})

MiscTab:CreateSlider({
	Name = "Jump Height",
	Range = {0, 1000},
	Increment = 1,
	CurrentValue = Settings.JumpHeight,
	Callback = function(v)
		Settings.JumpHeight = v
	end
})

MiscTab:CreateToggle({
	Name = "God Mode",
	CurrentValue = Settings.GodMode,
	Callback = function(v)
		Settings.GodMode = v
	end
})

MiscTab:CreateToggle({
	Name = "No Clip",
	CurrentValue = Settings.NoClip,
	Callback = function(v)
		Settings.NoClip = v
	end
})



local function Collect()
	local Remote = game:GetService("ReplicatedStorage").RemoteEvents.CollectMoney

	local slot = Settings.MacroCollect ~= "None" and Settings.MacroCollect or Settings.Collect
	if slot == "None" then return end

	-- NEVER SEND "All" TO SERVER
	if slot == "All" then
		for i = 1, 30 do
			Remote:FireServer("Slot" .. i)
			task.wait(0.03)
		end
	else
		Remote:FireServer(slot)
	end
end

local Dropdown = MainTab:CreateDropdown({
	Name = "Collect Slot",
	Options = {"None","Slot1","Slot2","Slot3","Slot4","Slot5","Slot6","Slot7","Slot8","Slot9","Slot10","Slot11","Slot12","Slot13","Slot14","Slot15","Slot16","Slot17","Slot18","Slot19","Slot20","Slot21","Slot22","Slot23","Slot24","Slot25","Slot26","Slot27","Slot28","Slot29","Slot30","All"},
	CurrentOption = {"Select One"},
	MultipleOptions = false,
	Callback = function(v)

		Settings.Collect = table.unpack(v)
		
	end,
})

local function Upgrade()
	local RF = game:GetService("ReplicatedStorage")
		:WaitForChild("RemoteFunctions")
		:WaitForChild("UpgradeBrainrot")

	local slot = Settings.MacroUpgrade ~= "None" and Settings.MacroUpgrade or Settings.Upgrade
	if slot == "None" then return end

	-- NEVER SEND "All" TO SERVER
	if slot == "All" then
		for i = 1, 30 do
			RF:InvokeServer("Slot" .. i)
			task.wait(0.05)
		end
	else
		RF:InvokeServer(slot)
	end
end

